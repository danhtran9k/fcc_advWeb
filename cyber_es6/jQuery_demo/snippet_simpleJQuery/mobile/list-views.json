{
	"jqmdivider": {
		"prefix": "jqmdivider",
		"body": "\r\n<li data-role=\"list-divider\">${1}</li>\r\n",
		"description": "jQuery Mobile: Listview divider",
		"scope": "text.html"
	},
	"jqmlistview:cb": {
		"prefix": "jqmlistview:cb",
		"body": "\r\n<ul data-role=\"listview\">\r\n\t<li><a href=\"${1:#}\">${2:item1}<span class=\"ui-li-count\">${3:1}</span></a></li>\r\n\t<li><a href=\"${4:#}\">${5:item2}<span class=\"ui-li-count\">${6:2}</span></a></li>\r\n\t<li><a href=\"${7:#}\">${8:item3}<span class=\"ui-li-count\">${9:3}</span></a></li>\r\n</ul>\r\n",
		"description": "jQuery Mobile: Listview with count bubbles",
		"scope": "text.html"
	},
	"jqmlistview:f": {
		"prefix": "jqmlistview:f",
		"body": "\r\n<ul data-role=\"listview\" data-filter=\"true\">\r\n\t<li><a href=\"${1:#}\">${2:item1}</a></li>\r\n\t<li><a href=\"${3:#}\">${4:item2}</a></li>\r\n\t<li><a href=\"${5:#}\">${6:item3}</a></li>\r\n</ul>\r\n",
		"description": "jQuery Mobile: Listview with filter",
		"scope": "text.html"
	},
